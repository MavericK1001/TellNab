{
  "name": "tellnab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:server:local": "ENV_FILE=config/local/server.env node server/index.js",
    "start:live": "NODE_ENV=production ENV_FILE=config/live/server.env node server/index.js",
    "dev:frontend:local": "vite --mode development",
    "build:frontend:live": "vite build --mode production",
    "smoke:advice": "node scripts/smoke-advice-flow.mjs",
    "smoke:wallet": "node scripts/smoke-wallet-badges-flow.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "prisma:generate": "prisma generate",
    "prisma:generate:live": "prisma generate --schema prisma/schema.live.prisma",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:dbpush:live": "prisma db push --schema prisma/schema.live.prisma",
    "prisma:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.21",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "vitest": "^0.34.0",
    "jsdom": "^24.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "prisma": "^5.22.0"
  }
}
